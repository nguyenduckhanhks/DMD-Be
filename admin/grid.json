        "redirectUrl": "/form/role/edit",
        "redirectUrlEmbed": {
          "id": "$id"
        }
      },
      {
        "icon": "plus",
        "color": "blue",
        "label": "Create",
        "action": "redirect",
        "position": "top",
        "redirectUrl": "/form/role/create"
      }
    ],
    "columns": [
      {
        "field": "id",
        "label": "ID",
        "display": "text"
      },
      {
        "field": "name",
        "label": "Name",
        "display": "text"
      }
    ],
    "filter": null
  },
