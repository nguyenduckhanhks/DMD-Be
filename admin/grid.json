[
  {
    "name": "user",
    "label": "User",
    "api": "/operation/api/get-users",
    "buttons": [
      {
        "icon": "plus",
        "label": "Create",
        "action": "redirect",
        "position": "top",
        "redirectUrl": "/form/user/create"
      },
      {
        "api": "/operation/block-user",
        "icon": "user close",
        "color": "red",
        "label": "Block",
        "action": "api",
        "position": "row",
        "hideExpression": {
          "$eq": ["$is_block", true]
        }
      },
      {
        "icon": "lock",
        "color": "red",
        "label": "Set password",
        "action": "redirect",
        "position": "row",
        "redirectUrl": "/form/password/create",
        "redirectUrlEmbed": {
          "embed": {
            "id": "$id"
          }
        }
      },
      {
        "api": "/operation/api/unblock-user",
        "icon": "user close",
        "color": "red",
        "label": "Unblock",
        "action": "api",
        "position": "row",
        "hideExpression": {
          "$eq": ["$is_block", false]
        }
      },
      {
        "icon": "edit",
        "color": "yellow",
        "label": "Edit",
        "action": "redirect",
        "position": "row",
        "redirectUrl": "/form/edit-user/edit",
        "redirectUrlEmbed": {
          "id": "$id"
        }
      }
    ],
    "columns": [
      {
        "field": "id",
        "label": "ID",
        "display": "text"
      },
      {
        "field": "user_name",
        "label": "User name",
        "display": "text"
      },
      {
        "field": "role",
        "label": "Permission",
        "display": "enum",
        "enumName": "user-role"
      },
      {
        "field": "is_block",
        "label": "Block",
        "display": "enum",
        "enumName": "yes-no"
      }
    ],
    "filter": []
  },
  {
    "name": "role",
    "label": "Role",
    "api": "/operation/api/get-role",
